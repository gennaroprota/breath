.   "$BREEZE_ROOT/tool/shell/new_line.shc"

(
start=0
finish=10

{
i=$start
while [ "$i" -lt "$finish" ]
do
    new_line "$i"  |  wc -l
    i=` expr "$i" + 1 `
done
}       >   output

{
i=$start
while [ "$i" -lt "$finish" ]
do
    printf '%s\n' "$i"
    i=` expr "$i" + 1 `
done
}       >   expected_output

)

